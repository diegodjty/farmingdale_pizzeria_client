{
	"name": "mongoose-deps-gen",
	"displayName": "Mongoose OS Dependency Generator",
	"description": "Generate and keep up to date Mongoose include paths for C/C++",
	"version": "0.1.3",
	"publisher": "alxandr",
	"engines": {
		"vscode": "^1.23.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/YoloDev/mongoose-deps-gen"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:mos-dep-gen.generate",
		"workspaceContains:mos.yml"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "mos-dep-gen.generate",
				"title": "Mongoose OS: Update include paths"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/execa": "^0.9.0",
		"@types/mkdirp": "^0.5.2",
		"@types/mocha": "^2.2.42",
		"@types/node": "^7.0.43",
		"tslint": "^5.8.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.6"
	},
	"dependencies": {
		"execa": "^0.10.0",
		"mkdirp": "^0.5.1"
	},
	"__metadata": {
		"id": "6bd2b0eb-77db-431d-98ba-88e9bd9ec94b",
		"publisherId": "33d4f3f7-4440-4d10-9989-ac23ac74382b",
		"publisherDisplayName": "Aleksander Heintz"
	}
}